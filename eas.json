{
  "cli": {
    "version": ">=16.10",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "channel": "base",
      "resourceClass": "medium"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "cicd": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "cicd"
    },
    "production": {
      "extends": "base",
      "environment": "production",
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "base": {
      "android": {
        "track": "internal",
        "changesNotSentForReview": true,
        "releaseStatus": "draft"
      },
      "ios": {
        "ascAppId": "---fill this in---",
        "appleTeamId": "---fill this in---"
      }
    },
    "production": {
      "extends": "base"
    }
  }
}
